<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemViewWithIcon">
        <attr name="item_icon" format="integer|reference"/>
        <attr name="item_text" format="string|reference"/>
        <attr name="item_text_detail" format="string|reference"/>
    </declare-styleable>

    <declare-styleable name="CollapseItemViewContainer">
        <attr name="item_title_icon" format="integer|reference"/>
        <attr name="item_title_name" format="string|reference"/>
        <attr name="item_title_detail" format="string|reference|integer"/>
    </declare-styleable>

    <declare-styleable name="CollapseItemView">
        <attr name="collapse_item_icon" format="integer|reference"/>
        <attr name="collapse_item_name" format="string|reference"/>
        <attr name="collapse_item_detail" format="string|reference|integer"/>
    </declare-styleable>
</resources>